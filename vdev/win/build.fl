import bob
import bob.cargo

let target = Cargo.build()
let lib_path = target + "libwin.so"

if Platform.os() == "Darwin" {
	lib_path = target + "libwin.dylib"
}

install = {
	lib_path: "lib/vdriver/aquabsd.black.win.vdriver",
}

run = none
